{
  "name": "expo-camera-issue",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "clean": "npx expo prebuild --clean",
    "doctor": "npx expo-doctor",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit -p . --pretty",
    "tidy": "yarn doctor && prettier --write \"**/*.{js,json,ts,tsx}\""
  },
  "dependencies": {
    "expo": "~50.0.17",
    "expo-av": "~13.10.6",
    "expo-camera": "15.0.2",
    "expo-device": "~5.9.4",
    "expo-haptics": "~12.8.1",
    "expo-image": "~1.10.6",
    "expo-status-bar": "~1.11.1",
    "expo-video-thumbnails": "~7.9.0",
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "expo": {
    "install": {
      "exclude": [
        "expo-camera"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "prettier": "^3.2.5",
    "typescript": "^5.1.3"
  },
  "private": true,
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
